{
  "name": "SJIFIRE",
  "version": "2.0.0",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy",
    "tina:dev": "tinacms dev -c \"npm run dev\"",
    "tina:build": "tinacms build && npm run build",
    "api:dev": "cd api && npm install && func start",
    "start": "npm-run-all --parallel dev api:dev",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint -c tests/eslint.config.mjs .eleventy.js src/modules/",
    "lint:css": "stylelint --config tests/stylelint.config.json src/css/**/*.css",
    "test": "playwright test --config=tests/playwright.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@azure/functions": "^4.10.0",
    "@azure/identity": "^4.13.0",
    "@azure/msal-node": "^3.8.4",
    "@octokit/rest": "^22.0.1",
    "@tinacms/cli": "^1.5.30",
    "clean-css": "^5.3.3",
    "jsonwebtoken": "^9.0.3",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "terser": "^5.44.1",
    "tinacms": "^3.1.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "azure-functions-core-tools": "^4.6.0",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1"
  }
}
