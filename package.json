{
  "name": "SJIFIRE",
  "version": "2.0.0",
  "scripts": {
    "stats": "node src/scripts/generate-stats.mjs",
    "sync-personnel": "node src/scripts/sync-personnel.mjs",
    "dev": "eleventy --serve",
    "build": "eleventy",
    "tina:dev": "TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"npm run dev\"",
    "tina:local-prod": "TINA_PUBLIC_IS_LOCAL=false TINA_PUBLIC_LOCAL_PROD=true tinacms dev -c \"npm run dev\"",
    "tina:build": "tinacms build && npm run build",
    "api:dev": "cd api && FUNCTIONS_WORKER_RUNTIME=node func start --verbose",
    "api:install": "cd api && npm install",
    "start": "npm-run-all --parallel dev api:dev",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint -c tests/eslint.config.mjs .eleventy.js src/_data/ src/_lib/ src/pages/",
    "lint:css": "stylelint --config tests/stylelint.config.json src/css/**/*.css",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "node --test tests/*.test.js",
    "test:e2e": "playwright test --config=tests/playwright.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/functions": "^4.10.0",
    "@azure/identity": "^4.13.0",
    "@azure/msal-node": "^3.8.4",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^22.0.1",
    "@tinacms/cli": "^2.0.7",
    "@tinacms/datalayer": "^2.0.5",
    "clean-css": "^5.3.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.3.1",
    "mongodb": "^7.0.0",
    "mongodb-level": "^0.0.4",
    "terser": "^5.44.1",
    "tinacms": "^3.2.0",
    "tinacms-gitprovider-github": "^4.0.5"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@azure/static-web-apps-cli": "^2.0.7",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "azure-functions-core-tools": "^4.6.0",
    "eslint": "^9.39.2",
    "npm-run-all": "^4.1.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1"
  }
}
